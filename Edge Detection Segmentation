      "cell_type": "code",
      "source": [
        "import scipy.ndimage as ndimage\n",
        "out_h = ndimage.convolve(gray_image, sobel_horizontal, mode='reflect')\n",
        "out_v = ndimage.convolve(gray_image, sobel_vertical, mode='reflect')\n",
        "# here mode determines how the input array is extended when the filter overlaps a border."
